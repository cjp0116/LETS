{
  "name": "GAINS",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.14.0",
    "@fullcalendar/core": "^5.10.0",
    "@fullcalendar/daygrid": "^5.9.0",
    "@fullcalendar/interaction": "^5.9.0",
    "@fullcalendar/react": "^5.10.0",
    "@fullcalendar/timegrid": "^5.9.0",
    "@glidejs/glide": "3.4.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "axios": "^0.21.4",
    "chart.js": "2.9.4",
    "choices.js": "9.0.1",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "jsonwebtoken": "^8.5.1",
    "moment": "2.27.0",
    "node-sass": "4.14.1",
    "nouislider": "14.6.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-bootstrap-sweetalert": "5.2.0",
    "react-datetime": "2.16.3",
    "react-dom": "16.13.1",
    "react-github-button": "0.1.11",
    "react-google-maps": "9.4.5",
    "react-notification-alert": "0.0.13",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "reactstrap": "8.5.1",
    "socket.io-client": "^4.2.0",
    "timeago.js": "^4.0.2"
  },
  "scripts": {
    "set-environment-variables": "REACT_APP_BASE_URL=https://workout-buddy-backend.herokuapp.com REACT_APP_PUBLIC_FOLDER=https://workout-buddy-backend.herokuapp.com/images/ npm run build",
    "reset-build": "cp build/index.html build/200.html && cd build/ && surge --domain descriptive-jellyfish.surge.sh",
    "start": "react-scripts start",
    "build": "react-scripts build && gulp build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/argon-design-system.scss src/assets/css/argon-design-system.css",
    "minify-sass": "node-sass src/assets/scss/argon-design-system.scss src/assets/css/argon-design-system.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-design-system.scss src/assets/css/argon-design-system.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "@types/googlemaps": "3.39.11",
    "@types/markerclustererplus": "2.1.33",
    "typescript": "3.9.7"
  }
}
